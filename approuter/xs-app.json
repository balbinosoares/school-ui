{
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/api(.*)$",
      "target": "$1",
      "destination": "srv_api",
      "authenticationType": "none"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "localDir": "../webapp"
    }

  ]
}